@model hallodoc_mvc_Repository.ViewModel.CreateShift<div class="modal fade" id="createshift" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">    <div class="modal-dialog modal-dialog-centered">        <div class="modal-content">            <div class="modal-header bg-info text-white">                <h1 class="modal-title fs-5" id="exampleModalLabel">Confirm Block</h1>                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>            </div>            <form asp-action="CreateShift" asp-controller="Physician" onsubmit="checkvalidation()">                <div class="modal-body">                    <select class="form-select mb-3" id="select" onchange="getphy(this.value)" aria-label="Default select example" asp-for="SelectedRegionId" disabled>                        <option selected disabled value="">@Model.Regionname</option>                                      </select>                    <div class="form-floating mb-3">                        <input type="date" class="form-control" id="shiftdate" min=@DateTime.Now.ToString("yyyy-MM-dd") asp-for="ShiftDate" />                        <label for="shiftdate">Shift Date</label>                    </div>                    <div class="row">                        <div class="col-lg-6 ">                            <div class="form-floating mb-3">                                <input type="time" class="form-control" id="start" asp-for="Start" />                                <label for="start">Start</label>                                <span id="start2" class="text-danger"></span>                            </div>                        </div>                        <div class="col-lg-6 ">                            <div class="form-floating mb-3 w-100">                                <input type="time" class="form-control" id="end" asp-for="End" onblur="displaynone()"/>                                <label for="end">End</label>                                <span id="end2" class="text-danger"></span>                            </div>                        </div>                    </div>                    <div class="form-check form-switch">                        <label class="form-check-label" for="repeat">Repeat</label>                        <input class="form-check-input" type="checkbox" id="repeat" asp-for="RepeatToggle" onclick="Enable(this)" />                    </div>                    <span class="mt-3">Repeat Days</span>                    <fieldset class="row mt-2 mb-3">                        <div class="col-5">                            <input class="form-check-input dis" type="checkbox" value="0" id="sunday" name="Weekday" disabled>                            <label class="form-check-label" for="sunday">Every Sunday</label>                        </div>                        <div class="col-5">                            <input class="form-check-input dis" type="checkbox" value="1" id="monday" name="Weekday" disabled>                            <label class="form-check-label" for="monday">Every Monday</label>                        </div>                        <div class="col-5">                            <input class="form-check-input dis" type="checkbox" value="2" id="tuesday" name="Weekday" disabled>                            <label class="form-check-label" for="tuesday">Every Tuesday</label>                        </div>                        <div class="col-5">                            <input class="form-check-input dis" type="checkbox" value="3" id="wednesday" name="Weekday" disabled>                            <label class="form-check-label" for="wednesday">Every Wednesday</label>                        </div>                        <div class="col-5">                            <input class="form-check-input dis" type="checkbox" value="4" id="thursday" name="Weekday" disabled>                            <label class="form-check-label" for="thursday">Every Thursday</label>                        </div>                        <div class="col-5">                            <input class="form-check-input dis" type="checkbox" value="5" id="friday" name="Weekday" disabled>                            <label class="form-check-label" for="friday">Every Friday</label>                        </div>                        <div class="col-5">                            <input class="form-check-input dis" type="checkbox" value="6" id="saturday" name="Weekday" disabled>                            <label class="form-check-label" for="saturday">Every Saturday</label>                        </div>                    </fieldset>                    <div class="form-floating">                        <select class="form-select mb-3 dis" id="repeatamt" aria-label="Default select example" asp-for="Repeat" disabled>                            <option selected value="1">1</option>                            <option value="2">2</option>                            <option value="3">3</option>                            <option value="4">4</option>                            <option value="5">5</option>                        </select>                        <label for="repeatamt">Repeat</label>                    </div>                </div>                <div class="modal-footer">                    <button type="submit" id="btn3" class="btn btn-outline-info mx-1 back-btn logout-btn"> Save </button>                    <button type="button" class="btn btn-outline-info mx-1 back-btn logout-btn" data-bs-dismiss="modal">                        Cancel                    </button>                </div>            </form>        </div>    </div></div><script>    function checkvalidation() {        var start = document.getElementById("start").value;        var end = document.getElementById("end").value;        var starthour = start.split(':')[0];        var startmin = start.split(':')[1];        var endhour = end.split(':')[0];        var endmin = end.split(':')[1];        if ((starthour == "00" && endhour == "00" && startmin == "00" && endmin == "00") || (starthour >= endhour)) {            document.getElementById("start2").innerHTML = "Start time is invalid";            document.getElementById("end2").innerHTML = "End time is invalid";            event.preventDefault();        }    }    function displaynone(){        var start = document.getElementById("start").value;        var end = document.getElementById("end").value;        var starthour = start.split(':')[0];        var startmin = start.split(':')[1];        var endhour = end.split(':')[0];        var endmin = end.split(':')[1];        if(endhour>starthour){            document.getElementById("start2").innerHTML = " ";            document.getElementById("end2").innerHTML = " ";        }    }    function Enable(ele) {        document.querySelectorAll(".dis").forEach(x => {            console.log(ele)            console.log(ele.checked)            if (ele.checked) {                x.removeAttribute("disabled");            }            else {                x.setAttribute("disabled", "");            }        })    }    function getphy(id) {        $.ajax({            url: '/Physician/GetPhysicianForShift',            data: { id: id },            success: function (data) {                if (data) {                    var content = "";                    if (data.length == 0) {                        console.log("break");                        content += `<option selected>Select Physician</option>`;                    }                    else if (Array.isArray(data)) {                        for (const item of data) {                            content += `<option value="${item.physicianId}">${item.firstName}</option>`;                        }                    } else {                        console.error('Unexpected data format: Received data is not an array of strings.');                    }                    $('#physician').html(content);                } else {                    var content = `<option disabled selected class="d-none"> Select Physician </option>`;                    $('#physicians').html(content);                }            },            error: function (jqXHR, textStatus, errorThrown) {                console.error('AJAX Error:', textStatus, errorThrown);            }        });    }</script>