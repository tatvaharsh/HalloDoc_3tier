@model hallodoc_mvc_Repository.ViewModel.Error@{    ViewData["Title"] = "Error";
  }<div class="main p-4">    <h1>Something is wrong...</h1>    <h4>Something's not quite right on our end! We're troubleshooting the issue and aim to have it fixed in a bit. Thanks for your patience while we work on it.</h4>    <p>Please contact us on www.tatvasoft.com</p>    <hr />    <div class="alert alert-danger">        <h5 class="alert-heading">Exception Path</h5>        <hr />        <p>@Model.Path</p>    </div>    <div class="alert alert-danger">        <h5 class="alert-heading">Exception Message</h5>        <hr />        <p>@Model.Message</p>    </div>    <div class="alert alert-danger">        <h5 class="alert-heading">Exception Stack Trace</h5>        <hr />        <p>@Model.Stack</p>    </div></div>