@{    Layout = ViewBag.Layout;}<div class="backdrop" id="backdrop" style="z-index:140"></div><div id="loader-container" class="vh-100 align-items-center justify-content-center" style="z-index:150">    <div class="loader" style="z-index:150"></div></div><div>    <div class="row" style="--bs-gutter-x: 0rem;">        <div class="col-12">            <div class="container mt-5 ps-1">                <h4 class="mt-3">Patient History</h4>            </div>            <div class="container rounded shadow py-3 mb-5">                <form class="mb-3">                    <div class="w-100  px-3 row">                        <div class="form-floating col-md-3 col-sm-6 mt-1">                            <input type="text" class="form-control" id="fname" placeholder="First Name" />                            <label for="fname" class="form-label ms-2">First Name</label>                        </div>                        <div class="form-floating col-md-3 col-sm-6 mt-1">                            <input type="text" class="form-control" id="lname" placeholder="Last Name" />                            <label for="lname" class="form-label ms-2">Last Name</label>                        </div>                        <div class="form-floating col-md-3 col-sm-6 mt-1">                            <input type="text" class="form-control" id="email" placeholder="Email" />                            <label for="email" class="form-label ms-2">Email</label>                        </div>                        <div class="form-floating col-md-3 col-sm-6 mt-1">                            <input type="text" class="form-control" id="phoneno" placeholder="Phone Number" />                            <label for="phoneno" class="form-label ms-2">Phone Number</label>                        </div>                    </div>                    <div class="row mt-3 me-3">                        <div class="col text-end">                            <button class="btn btn-outline-info me-2" type="reset">Clear</button>                            <button class=" btn btn-info text-white" type="button" onclick="filter2()">Search</button>                        </div>                    </div>                </form>                <div id="tableContent" class="w-100">                </div>            </div>        </div>    </div></div><script src="https://code.jquery.com/jquery-3.6.0.min.js"></script><script>    const navLinks = document.querySelectorAll('.nav-link');    const provider = document.getElementById("Records-tab");    navLinks.forEach(link => {        link.classList.remove('active');    });    provider.classList.add('active');</script><script>    $(document).ready(function () {        $.ajax({            url: "/Admin/PatientHistoryTable",            type: "GET",            data: { fname: null, lname: null, email: null, phone: null,page:0 },            dataType: "html",            success: function (data) {                $("#tableContent").html(data);            },            error: function () {                alert("No Projects Found");                $("#tableContent").html('An error has occurred');            }        });    });</script><script>    function filter2(page) {        let fname = document.getElementById('fname').value;        let lname = document.getElementById('lname').value;        let email = document.getElementById('email').value;        let phone = document.getElementById('phoneno').value;        $.ajax({            url: "/Admin/PatientHistoryTable",            type: "GET",            data: { fname: fname, lname: lname, email: email, phone: phone,page:page },            dataType: "html",            success: function (data) {                $("#tableContent").html(data);            },            error: function () {                alert("No Projects Found");                $("#tableContent").html('An error has occurred');            }        });    }</script>