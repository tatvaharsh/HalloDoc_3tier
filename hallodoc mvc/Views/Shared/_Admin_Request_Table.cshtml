        background-color:black;
    }*/
  
 </style>
@*<script src="~/js/AdminDashboard.js"></script>*@




    <table class="table border-4 d-none d-lg-table" id="adminTable">
        @if (Model.FirstOrDefault() != null)
        {
            <thead class="">
                <tr>
                    <th scope="col">Name</th>
                    <th scope="col"></th>

                    @if (@Model.FirstOrDefault().Status == 1 || @Model.FirstOrDefault().Status == 2 || @Model.FirstOrDefault().Status == 3 ||
                  @Model.FirstOrDefault().Status == 4 || @Model.FirstOrDefault().Status == 5 || @Model.FirstOrDefault().Status == 6 ||
                  @Model.FirstOrDefault().Status == 7 || @Model.FirstOrDefault().Status == 8)
                    {
                        <th scope="col">Date Of Birth</th>
                    }

                    @if (@Model.FirstOrDefault().Status == 3 || @Model.FirstOrDefault().Status == 7 || @Model.FirstOrDefault().Status == 8)
                    {
                        <th scope="col">Region</th>
                    }
                    @if (@Model.FirstOrDefault().Status == 5 || @Model.FirstOrDefault().Status == 1 || @Model.FirstOrDefault().Status == 2 || @Model.FirstOrDefault().Status == 4)
                    {
                        <th scope="col">Requestor</th>

                    }

                    @if (@Model.FirstOrDefault().Status == 2 || @Model.FirstOrDefault().Status == 3 || @Model.FirstOrDefault().Status == 4 || @Model.FirstOrDefault().Status == 5 ||
                  @Model.FirstOrDefault().Status == 6 ||
                  @Model.FirstOrDefault().Status == 7 || @Model.FirstOrDefault().Status == 8 || @Model.FirstOrDefault().Status == 9)
                    {
                        <th scope="col">Physician Name</th>
                        <th scope="col">Date Of Service</th>
                    }

                    @if (@Model.FirstOrDefault().Status == 1)
                    {
                        <th scope="col">Requested Date</th>
                    }

                    @if (@Model.FirstOrDefault().Status == 1 || @Model.FirstOrDefault().Status == 2 || @Model.FirstOrDefault().Status == 4 || @Model.FirstOrDefault().Status == 5 || @Model.FirstOrDefault().Status == 6)
                    {
                        <th scope="col">Phone</th>
                    }

                    <th scope="col">Address</th>

                    @if (@Model.FirstOrDefault().Status == 1 || @Model.FirstOrDefault().Status == 2 || @Model.FirstOrDefault().Status == 3 || @Model.FirstOrDefault().Status == 4 || @Model.FirstOrDefault().Status == 7 || @Model.FirstOrDefault().Status == 8)
                    {
                        <th scope="col">Notes</th>
                    }

                    <th scope="col">Chat With</th>
                    <th scope="col">Actions</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var obj in Model)
                {
                    <tr class="align-middle table-@{
                    @GetTableColorClass(obj.RequestTypeId)}">

                        <td scope="row">
                            @obj.Name
                        </td>
                        <td scope="row">
                            <a class="btn btn-outline-light p-1" style="font-size:smaller">
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-envelope" viewBox="0 0 16 16">
                                    <path d="M0 4a2 2 0 0 1 2-2h12a2 2 0 0 1 2 2v8a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2zm2-1a1 1 0 0 0-1 1v.217l7 4.2 7-4.2V4a1 1 0 0 0-1-1zm13 2.383-4.708 2.825L15 11.105zm-.034 6.876-5.64-3.471L8 9.583l-1.326-.795-5.64 3.47A1 1 0 0 0 2 13h12a1 1 0 0 0 .966-.741M1 11.105l4.708-2.897L1 5.383z" />
                                </svg>
                            </a>
                        </td>

                        @if (@Model.FirstOrDefault().Status == 1 || @Model.FirstOrDefault().Status == 2 || @Model.FirstOrDefault().Status == 3 ||
                     @Model.FirstOrDefault().Status == 4 || @Model.FirstOrDefault().Status == 5 || @Model.FirstOrDefault().Status == 6 ||
                     @Model.FirstOrDefault().Status == 7 || @Model.FirstOrDefault().Status == 8)
                        {
                            <td>
                                @obj.DateOfBirth
                            </td>
                        }

                        @if (@Model.FirstOrDefault().Status == 3 || @Model.FirstOrDefault().Status == 7 || @Model.FirstOrDefault().Status == 8)
                        {
                            <td>
                                @obj.Region
                            </td>
                        }
                        @if (@Model.FirstOrDefault().Status == 5 || @Model.FirstOrDefault().Status == 1 || @Model.FirstOrDefault().Status == 2 || @Model.FirstOrDefault().Status == 4)
                        {
                            <td>
                                @if (obj.RequestTypeId == 2)
                                {
                                    <span>(Patient)</span>
                                    @obj.Requestor
                                }
                                @if (obj.RequestTypeId == 3)
                                {
                                    <span>(Family/Friend)</span>
                                    @obj.Requestor
                                }
                                @if (obj.RequestTypeId == 4)
                                {
                                    <span>(Concierge)</span>
                                    @obj.Requestor
                                }
                                @if (obj.RequestTypeId == 1)
                                {
                                    <span>(Business)</span>
                                    @obj.Requestor
                                }
                               
                            </td>
                        }

                        @if (@Model.FirstOrDefault().Status == 2 || @Model.FirstOrDefault().Status == 3 || @Model.FirstOrDefault().Status == 4 || @Model.FirstOrDefault().Status == 5 ||
                     @Model.FirstOrDefault().Status == 6 ||
                     @Model.FirstOrDefault().Status == 7 || @Model.FirstOrDefault().Status == 8 || @Model.FirstOrDefault().Status == 9)
                        {
                            <td>
                                @obj.Physician
                            </td>

                        }


                        @if (@Model.FirstOrDefault().Status == 2 || @Model.FirstOrDefault().Status == 3 || @Model.FirstOrDefault().Status == 4 || @Model.FirstOrDefault().Status == 5 ||
                     @Model.FirstOrDefault().Status == 6 ||
                     @Model.FirstOrDefault().Status == 7 || @Model.FirstOrDefault().Status == 8 || @Model.FirstOrDefault().Status == 9)
                        {
                            <td>
                                DOS
                            </td>
                        }

                        @if (@Model.FirstOrDefault().Status == 1)
                        {
                            <td>
                                @obj.RequestDate
                            </td>
                        }
                        @if (@Model.FirstOrDefault().Status == 1 || @Model.FirstOrDefault().Status == 2 || @Model.FirstOrDefault().Status == 4 || @Model.FirstOrDefault().Status == 5 || @Model.FirstOrDefault().Status == 6)
                        {
                            <td>
                                <a class="btn btn-outline-light p-1 d-flex" style="font-size:smaller">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="16"  height="16" fill="currentColor" class="bi bi-telephone-fill mt-1" viewBox="0 0 16 16">
                                        <path fill-rule="evenodd" d="M1.885.511a1.745 1.745 0 0 1 2.61.163L6.29 2.98c.329.423.445.974.315 1.494l-.547 2.19a.68.68 0 0 0 .178.643l2.457 2.457a.68.68 0 0 0 .644.178l2.189-.547a1.75 1.75 0 0 1 1.494.315l2.306 1.794c.829.645.905 1.87.163 2.611l-1.034 1.034c-.74.74-1.846 1.065-2.877.702a18.6 18.6 0 0 1-7.01-4.42 18.6 18.6 0 0 1-4.42-7.009c-.362-1.03-.037-2.137.703-2.877z" />
                                    </svg>
                                    @obj.Phone
                                </a>
                                (Patient)
                                @if (obj.RequestTypeId == 1 && obj.RPhone != null)
                                {
                                    <a class="btn btn-outline-light p-1 d-flex" style="font-size:smaller">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-telephone-fill mt-1" viewBox="0 0 16 16">
                                            <path fill-rule="evenodd" d="M1.885.511a1.745 1.745 0 0 1 2.61.163L6.29 2.98c.329.423.445.974.315 1.494l-.547 2.19a.68.68 0 0 0 .178.643l2.457 2.457a.68.68 0 0 0 .644.178l2.189-.547a1.75 1.75 0 0 1 1.494.315l2.306 1.794c.829.645.905 1.87.163 2.611l-1.034 1.034c-.74.74-1.846 1.065-2.877.702a18.6 18.6 0 0 1-7.01-4.42 18.6 18.6 0 0 1-4.42-7.009c-.362-1.03-.037-2.137.703-2.877z" />
                                        </svg>
                                        @obj.RPhone
                                    </a>
                                    <span>(Business)</span>
                                }
                                @if (obj.RequestTypeId == 3 && obj.RPhone != null)
                                {
                                    <a class="btn btn-outline-light p-1 d-flex" style="font-size:smaller">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-telephone-fill mt-1" viewBox="0 0 16 16">
                                            <path fill-rule="evenodd" d="M1.885.511a1.745 1.745 0 0 1 2.61.163L6.29 2.98c.329.423.445.974.315 1.494l-.547 2.19a.68.68 0 0 0 .178.643l2.457 2.457a.68.68 0 0 0 .644.178l2.189-.547a1.75 1.75 0 0 1 1.494.315l2.306 1.794c.829.645.905 1.87.163 2.611l-1.034 1.034c-.74.74-1.846 1.065-2.877.702a18.6 18.6 0 0 1-7.01-4.42 18.6 18.6 0 0 1-4.42-7.009c-.362-1.03-.037-2.137.703-2.877z" />
                                        </svg>
                                        @obj.RPhone
                                    </a>
                                    <span>(Relative/Family)</span>
                                }
                                @if (obj.RequestTypeId == 4 && obj.RPhone != null)
                                {
                                    <a class="btn btn-outline-light p-1 d-flex" style="font-size:smaller">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-telephone-fill mt-1" viewBox="0 0 16 16">
                                            <path fill-rule="evenodd" d="M1.885.511a1.745 1.745 0 0 1 2.61.163L6.29 2.98c.329.423.445.974.315 1.494l-.547 2.19a.68.68 0 0 0 .178.643l2.457 2.457a.68.68 0 0 0 .644.178l2.189-.547a1.75 1.75 0 0 1 1.494.315l2.306 1.794c.829.645.905 1.87.163 2.611l-1.034 1.034c-.74.74-1.846 1.065-2.877.702a18.6 18.6 0 0 1-7.01-4.42 18.6 18.6 0 0 1-4.42-7.009c-.362-1.03-.037-2.137.703-2.877z" />
                                        </svg>
                                        @obj.RPhone
                                    </a>
                                    <span>(Concierge) </span>
                                }
                            </td>
                        }
                        <td>
                            @obj.Address
                        </td>

                        @if (@Model.FirstOrDefault().Status == 1 || @Model.FirstOrDefault().Status == 2 || @Model.FirstOrDefault().Status == 3 || @Model.FirstOrDefault().Status == 4 || @Model.FirstOrDefault().Status == 7 || @Model.FirstOrDefault().Status == 8)
                        {
                        <td>
                            @{
                                int count = 1;
                            }
                            @foreach (var x in obj.Notes)
                            {   
                                <div>@count) @x</div>
                                count++;
                            }
                        </td>
                        }

                        <td>
                            @if (@obj.ChatWith != null)
                            {
                                <button class="btn btn-outline-light" type="button">Provider</button>
                            }
                        </td>

                        <td>
                            <div class="dropdown ">
                                <button class="btn btn-outline-light dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false">
                                    Actions
                                </button>
                                <ul class="dropdown-menu">

                                    @if (Model.FirstOrDefault().Status == 1)
                                    {
                                        <li><a class="dropdown-item" onclick="assignCase(@obj.Id)">Assign Case</a></li>
                                    }
                                    @if (Model.FirstOrDefault().Status == 1)
                                    {
                                        <li><a class="dropdown-item" onclick="cancelCase(@obj.Id)">Cancle Case</a></li>
                                    }
                                    <li><a class="dropdown-item"  asp-action="ViewCase" asp-route-id="@obj.Id" >View Case</a></li>
                                    <li><a class="dropdown-item" asp-action="ViewNotes" asp-route-id="@obj.Id">View Notes</a></li>
                                    @if (Model.FirstOrDefault().Status == 1)
                                    {
                                        <li><a class="dropdown-item"  onclick="blockCase(@obj.Id)">Block Patient</a></li>
                                    }
                                    @if (Model.FirstOrDefault().Status != 1)
                                    {
                                    <li><a class="dropdown-item" asp-action="ViewUpload" asp-route-id="@obj.Id">View Uploads</a></li>
                                }
                                    @if (Model.FirstOrDefault().Status == 2)
                                    {
                                    <li><a class="dropdown-item" onclick="TransferCase(@obj.Id)">Transfer</a></li>
                                }
                                    @if (Model.FirstOrDefault().Status == 2 || Model.FirstOrDefault().Status == 3 || Model.FirstOrDefault().Status == 7 || Model.FirstOrDefault().Status == 8)
                                    {
                                    <li><a class="dropdown-item" onclick="Clear(@obj.Id)">Clear Case</a></li>
                                    }
                                    @if (Model.FirstOrDefault().Status == 2)
                                    {
                                    <li><a class="dropdown-item" onclick="SendAgreement(@obj.Id ,@obj.RequestTypeId)">Send Agreement</a></li>
                                }
                                    @if (Model.FirstOrDefault().Status == 3 || Model.FirstOrDefault().Status == 4 || Model.FirstOrDefault().Status == 5 || Model.FirstOrDefault().Status == 6 || Model.FirstOrDefault().Status == 7 || Model.FirstOrDefault().Status == 3) 
                                    {
                                    <li><a class="dropdown-item" asp-action="Order" asp-route-id="@obj.Id">Orders</a></li>
                                }
                                    @if (Model.FirstOrDefault().Status == 3 || Model.FirstOrDefault().Status == 4 || Model.FirstOrDefault().Status == 5)
                                    {
                                        <li><a class="dropdown-item" href="#">Docter Notes</a></li>
                                    }
                                    @if (Model.FirstOrDefault().Status == 3 || Model.FirstOrDefault().Status == 4 || Model.FirstOrDefault().Status == 5)
                                    {
                                        <li><a class="dropdown-item" href="#">Encounter</a></li>
                                    }
                                    @if (Model.FirstOrDefault().Status == 3)
                                    {
                                    <li><a class="dropdown-item" asp-action="Close" asp-route-id="@obj.Id">Close Case</a></li>
                                    }
                                </ul>
                            </div>

                        </td>
                    </tr>
                }
            </tbody>
        }
    </table>






@*_______________________________accordian-----------------------------------------------------------------------*@

<div class="mt-2 border rounded d-block d-lg-none">
 @*   <form role="search" class=" d-flex align-items-center border border-rounded rounded border-info mb-2">
        <i type="search" class="bi bi-search px-2"></i>
        <input class="form-control searchfocus border-0 h-auto" type="search" id="my-search-input" placeholder="Search" aria-label="Search">
    </form>*@
   
    <div class="accordion container-fluid  p-0" id="accordionPanelsStayOpenExample"></div>
    <div class="accordion accordion-flush" id="accordionFlushExample">
        @foreach (var item in Model)
        {

            TimeSpan t = DateTime.Now - item.RequestDate;
            int hours = (int)t.TotalHours;
            string id = 'a' + item.Id.ToString();

            <div class="accordion-item">
                <h2 class="accordion-header" id="flush-heading"></h2>
                <div class="accordion-button collapsed d-flex flex-column" style="background-color: #00bae920;text-decoration: none !important;position: relative;" data-bs-toggle="collapse" data-bs-target="#@id" aria-expanded="true" aria-controls="flush-collapse">

                    <div class="w-100 d-flex justify-content-between m-1">
                        <div class="">@item.LName, @item.FName</div>
                        <div class="">
                            @if (item.RequestTypeId == 1)
                            {
                                <span>Business</span>
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" style="color: #c122a1;" fill="currentColor" class="bi bi-circle-fill" viewBox="0 0 16 16">
                                    <circle cx="8" cy="8" r="8" />
                                </svg>
                                
                            }
                            else if (item.RequestTypeId == 2)
                            {
                                <span>Patient</span>
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" style="color: #23a727;" fill="currentColor" class="bi bi-circle-fill" viewBox="0 0 16 16">
                                    <circle cx="8" cy="8" r="8" />
                                </svg>
                               
                            }
                            else if (item.RequestTypeId == 3)
                            {
                                <span>Family / Friend</span>
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" style="color: orange;" fill="currentColor" class="bi bi-circle-fill" viewBox="0 0 16 16">
                                    <circle cx="8" cy="8" r="8" />
                                </svg>
                               
                            }
                            else
                            {
                                <span>Concierge</span>
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" style="color: #0d1dac;" fill="currentColor" class="bi bi-circle-fill" viewBox="0 0 16 16">
                                    <circle cx="8" cy="8" r="8" />
                                </svg>
                              
                            }
                        </div>

                    </div>
                    <div class="w-100 d-flex justify-content-between m-1 align-items-center">
                        <div class="">@item.Address</div>
                        <div class="">
                            <button class="btn text-nowrap rounded-pill btn-outline-info z-3" type="button">
                                <small>Map Location</small>
                            </button>
                        </div>
                    </div>




                    <div class="w-100 d-flex justify-content-between m-1">
                        <div class="">
                            <i class="bi bi-clock text-info pe-1"></i>
                            Wait Time : <strong>@item.RequestDate (@hours:@t.Minutes)</strong>
                        </div>
                    </div>
                </div>

                <hr class="m-0 p-0 text-muted" />
                <div id="@id" class="accordion-collapse collapse" style="background-color: #00bae920;" aria-labelledby="flush-heading" data-bs-parent="#accordionFlushExample">
                    <div class="d-flex justify-content-end">
                        <a role="button" asp-action="ViewCase" asp-route-id="@item.Id" class="btn rounded-pill border-secondary text-dark z-3 mx-3" style="background-color:rgba(255, 255, 255, 0.719);position: relative;bottom: 20px;right: 5px; z-index: 1050;" type="button">
                            View
                            Case
                        </a>
                    </div>
                    <div class="accordion-body main-flex">
                     
                        <div class="">
                            @if (item.Status == 1)
                            {
                                <div class="inner-flex">
                                    <i class="bi bi-calendar-date rborder p-2" style="font-size: 13px;"></i>
                                    <span class="accordion-heading-inner ms-2">
                                        Date of Birth:
                                        <span class="accordion-value-inner">
                                            @item.DateOfBirth
                                        </span>
                                    </span>
                                </div>
                                <div class="inner-flex mt-3">
                                    <i class="bi bi-envelope rborder p-2" style="font-size: 13px;"></i>
                                    <span class="accordion-heading-inner ms-2">
                                        Email:
                                        <span class="accordion-value-inner">
                                            @item.Email
                                        </span>
                                    </span>
                                </div>
                                <div class="inner-flex mt-3">
                                    <i class="bi bi-telephone rborder p-2" style="font-size: 13px;"></i>
                                    <span class="accordion-heading-inner ms-2">
                                        Patient:
                                        <span class="accordion-value-inner">
                                            @item.Phone
                                        </span>
                                    </span>
                                </div>
                                <div class="inner-flex mt-3">
                                    <i class="bi bi-person rborder p-2" style="font-size: 13px;"></i>
                                    <span class="accordion-heading-inner ms-2">
                                        Requestor: @item.Requestor
                                        <span class="accordion-value-inner">
                                            @if (item.RequestTypeId == 1)
                                            {
                                                <span>Business</span>

                                            }
                                            else if (item.RequestTypeId == 2)
                                            {
                                                <span>Patient</span>

                                            }
                                            else if (item.RequestTypeId == 3)
                                            {
                                                <span>Family / Friend</span>

                                            }
                                            else
                                            {
                                                <span>Concierge</span>

                                            }
                                        </span>
                                    </span>
                                </div>
                                <div class="mt-3 row justify-content-between">
                                    <button class="col p-2 m-1 btn rounded-pill  text-white b1" onclick="assignCase(@item.Id)" data-bs-toggle="modal" data-bs-target="#myModalAssign" type="button" style="background-color: #9f22c1;">
                                        Assign Case
                                    </button>
                                    <button class="col p-2 m-1 btn rounded-pill btn-danger b1" onclick="cancelCase(@item.Id)" type="button">
                                        Cancel Case
                                    </button>
                                </div>

                                <div class="row justify-content-between mt-2">
                                    <a class="col p-2 m-1 btn rounded-pill btn-success b1" asp-action="ViewNotes" asp-route-id="@item.Id" style="background-color: #23a727;">
                                        View Notes
                                    </a>
                                    <button class="col p-2 m-1 btn rounded-pill btn-danger b1" onclick="blockCase(@item.Id)" type="button">
                                        Block Patient
                                    </button>
                                </div>
                                <div class="row justify-content-between mt-2">
                                    <button class="col-6 p-2 m-1 btn rounded-pill btn-success b1" type="button" style="background-color: #23a727;">
                                        Email
                                    </button>
                                </div>
                            }
                            @if (item.Status == 2)
                            {
                                <div class="inner-flex">
                                    <i class="bi bi-calendar-date rborder p-2" style="font-size: 13px;"></i>
                                    <span class="accordion-heading-inner ms-2">
                                        Date of Birth:
                                        <span class="accordion-value-inner">
                                            @item.DateOfBirth
                                        </span>
                                    </span>
                                </div>
                                <div class="inner-flex mt-3">
                                    <i class="bi bi-envelope rborder p-2" style="font-size: 13px;"></i>
                                    <span class="accordion-heading-inner ms-2">
                                        Email:
                                        <span class="accordion-value-inner">
                                            @item.Email
                                        </span>
                                    </span>
                                </div>
                                <div class="inner-flex mt-3">
                                    <i class="bi bi-telephone rborder p-2" style="font-size: 13px;"></i>
                                    <span class="accordion-heading-inner ms-2">
                                        Patient:
                                        <span class="accordion-value-inner">
                                            @item.Phone
                                        </span>
                                    </span>
                                </div>
                                <div class="inner-flex mt-3">
                                    <i class="bi bi-telephone rborder p-2" style="font-size: 13px;"></i>
                                    <span class="accordion-heading-inner ms-2">
                                        Transfer Notes:
                                        <span class="accordion-value-inner">
                                            @{
                                                int count = 1;
                                            }
                                            @foreach (var x in item.Notes)
                                            {
                                                <span>@count) @x</span>
                                                count++;
                                            }
                                        </span>
                                    </span>
                                </div>
                                <div class="inner-flex mt-3">
                                    <i class="bi bi-telephone rborder p-2" style="font-size: 13px;"></i>
                                    <span class="accordion-heading-inner ms-2">
                                        Date of Service:
                                        <span class="accordion-value-inner">
                                           DOS
                                        </span>
                                    </span>
                                </div>
                                <div class="inner-flex mt-3">
                                    <i class="bi bi-telephone rborder p-2" style="font-size: 13px;"></i>
                                    <span class="accordion-heading-inner ms-2">
                                       Physician:
                                        <span class="accordion-value-inner">
                                            @item.Physician
                                        </span>
                                    </span>
                                </div>
                                <div class="inner-flex mt-3">
                                    <i class="bi bi-person rborder p-2" style="font-size: 13px;"></i>
