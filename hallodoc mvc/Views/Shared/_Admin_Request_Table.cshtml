
@model IEnumerable<hallodoc_mvc_Repository.ViewModel.AdminDashboard>
@functions {
    public string GetTableColorClass(int RequstTypeId)
    {
        switch (RequstTypeId)
        {
            case 1: return "pink";
            case 2: return "green";
            case 3: return "orange";
            case 4: return "blue";
            default: return "green";
        }
    }
    public string okok(int Status)
    {
        switch (Status)
        {
            case 1: return "New";
            case 2: return "Pending";
            case 3: return "Close";
            case 4: return "Active";
            case 5: return "Active";
            case 6: return "Conclude";
            case 7: return "Close";
            case 8: return "Close";
            case 9: return "Unpaid";
            default: return "New";
        }
    }
}
<style>
    .bi-search::before{
        display:none;
    }

    .searchfocus{
        padding:0px;    
    }
