@model List<hallodoc_mvc_Repository.ViewModel.Emaillogs><table class="table d-none d-md-table">    <thead>        <tr class="text-center align-middle" style="background-color:gray">            <th>Recipient</th>            <th>Action</th>            <th>Role Name</th>            <th>Email Id</th>            <th>Created Date</th>            <th>Sent Date</th>            <th>Sent</th>            <th>Sent Tries</th>            <th>Confirmation Number</th>        </tr>    </thead>    <tbody>        @foreach (var item in Model)
        {            <tr class="text-center align-middle">                <td>@item.Recipient</td>                <td>                </td>                <td>@item.RoleName</td>                <td>@item.Email</td>                <td>@item.CreatedDate.ToString("MMM dd, yyyy")</td>                <td>@(item.SentDate.HasValue ? item.SentDate.Value.ToString("MMM dd, yyyy") : "-")</td>                <td>                    @if (item.IsSent ?? false)                    {                        <span>Yes</span>                    }                    else                    {                        <span>No</span>                    }                </td>                <td>@item.SentTries</td>                <td>                    @if (item.ConfirmationNumber != null)                    {                        <span>@item.ConfirmationNumber.ToUpper()</span>                    }                    else                    {                        <span>-</span>                    }                </td>            </tr>        }    </tbody></table><div class="accordion accordion-flush d-md-none mt-3" id="accordionFlushExample">    @foreach (var item in Model)
    {

        string Id = 'a' + @item.EmailLogId.ToString();        <div class="accordion-item">            <h2 class="accordion-header">                <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#@Id" aria-expanded="false" aria-controls="flush-collapseOne">                    <div class="d-block">                        <p class="fw-bold">@item.Recipient</p>                        <p>Action: </p>                        <p>Email: @item.Email</p>                    </div>                </button>            </h2>            <div id="@Id" class="accordion-collapse collapse" data-bs-parent="#accordionFlushExample">                <div class="accordion-body">                    <div class="mb-2">                        <svg xmlns="http://www.w3.org/2000/svg" width="23" height="23" fill="currentColor" class="bi bi-person-check text-info" viewBox="0 0 20 20">                            <path d="M12.5 16a3.5 3.5 0 1 0 0-7 3.5 3.5 0 0 0 0 7m1.679-4.493-1.335 2.226a.75.75 0 0 1-1.174.144l-.774-.773a.5.5 0 0 1 .708-.708l.547.548 1.17-1.951a.5.5 0 1 1 .858.514M11 5a3 3 0 1 1-6 0 3 3 0 0 1 6 0M8 7a2 2 0 1 0 0-4 2 2 0 0 0 0 4" />                            <path d="M8.256 14a4.5 4.5 0 0 1-.229-1.004H3c.001-.246.154-.986.832-1.664C4.484 10.68 5.711 10 8 10q.39 0 .74.025c.226-.341.496-.65.804-.918Q8.844 9.002 8 9c-5 0-6 3-6 4s1 1 1 1z" />                        </svg>                        <span> Role: @item.RoleName </span>                    </div>                    <div class="mb-2">                        <svg xmlns="http://www.w3.org/2000/svg" width="23" 20height="23" fill="currentColor" class="bi bi-calendar-check text-info" viewBox="0 0 20 20">                            <path d="M10.854 7.146a.5.5 0 0 1 0 .708l-3 3a.5.5 0 0 1-.708 0l-1.5-1.5a.5.5 0 1 1 .708-.708L7.5 9.793l2.646-2.647a.5.5 0 0 1 .708 0" />                            <path d="M3.5 0a.5.5 0 0 1 .5.5V1h8V.5a.5.5 0 0 1 1 0V1h1a2 2 0 0 1 2 2v11a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V3a2 2 0 0 1 2-2h1V.5a.5.5 0 0 1 .5-.5M1 4v10a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1V4z" />                        </svg>                        <span> Created Date: @item.CreatedDate</span>                    </div>                    <div class="mb-2">                        <svg xmlns="http://www.w3.org/2000/svg" width="23" 20height="23" fill="currentColor" class="bi bi-calendar-check text-info" viewBox="0 0 20 20">                            <path d="M10.854 7.146a.5.5 0 0 1 0 .708l-3 3a.5.5 0 0 1-.708 0l-1.5-1.5a.5.5 0 1 1 .708-.708L7.5 9.793l2.646-2.647a.5.5 0 0 1 .708 0" />                            <path d="M3.5 0a.5.5 0 0 1 .5.5V1h8V.5a.5.5 0 0 1 1 0V1h1a2 2 0 0 1 2 2v11a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V3a2 2 0 0 1 2-2h1V.5a.5.5 0 0 1 .5-.5M1 4v10a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1V4z" />                        </svg>                        <span>  Sent Date: @(item.SentDate.HasValue ? item.SentDate.Value.ToString("MMM dd, yyyy") : "-")</span>                    </div>                    <div class="mb-2">                        <svg xmlns="http://www.w3.org/2000/svg" width="23" height="23" fill="currentColor" class="bi bi-check-circle text-info" viewBox="0 0 20 20">                            <path d="M8 15A7 7 0 1 1 8 1a7 7 0 0 1 0 14m0 1A8 8 0 1 0 8 0a8 8 0 0 0 0 16" />                            <path d="m10.97 4.97-.02.022-3.473 4.425-2.093-2.094a.75.75 0 0 0-1.06 1.06L6.97 11.03a.75.75 0 0 0 1.079-.02l3.992-4.99a.75.75 0 0 0-1.071-1.05" />                        </svg>                        <span>                            Sent: @if (item.IsSent ?? false)
                            {                                <span>Yes</span>                            }
                            else
                            {                                <span>No</span>                            }                        </span>                    </div>                    <div class="mb-2">                        <svg xmlns="http://www.w3.org/2000/svg" width="23" height="23" fill="currentColor" class="bi bi-envelope text-info" viewBox="0 0 20 20">                            <path d="M0 4a2 2 0 0 1 2-2h12a2 2 0 0 1 2 2v8a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2zm2-1a1 1 0 0 0-1 1v.217l7 4.2 7-4.2V4a1 1 0 0 0-1-1zm13 2.383-4.708 2.825L15 11.105zm-.034 6.876-5.64-3.471L8 9.583l-1.326-.795-5.64 3.47A1 1 0 0 0 2 13h12a1 1 0 0 0 .966-.741M1 11.105l4.708-2.897L1 5.383z" />                        </svg>                        <span> Sent Tries: @item.SentTries</span>                    </div>                    <div class="mb-2">                        <svg xmlns="http://www.w3.org/2000/svg" width="23" height="23" fill="currentColor" class="bi bi-check-circle text-info" viewBox="0 0 20 20">                            <path d="M8 15A7 7 0 1 1 8 1a7 7 0 0 1 0 14m0 1A8 8 0 1 0 8 0a8 8 0 0 0 0 16" />                            <path d="m10.97 4.97-.02.022-3.473 4.425-2.093-2.094a.75.75 0 0 0-1.06 1.06L6.97 11.03a.75.75 0 0 0 1.079-.02l3.992-4.99a.75.75 0 0 0-1.071-1.05" />                        </svg>                        <span>                            Sent: @if (item.ConfirmationNumber != null)
                            {                                <span>@item.ConfirmationNumber.ToUpper()</span>                            }
                            else
                            {                                <span>-</span>                            }                        </span>                    </div>                </div>            </div>        </div>    }</div>