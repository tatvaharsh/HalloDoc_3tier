@model hallodoc_mvc_Repository.ViewModel.PartnersCM@{    Layout = "_LayAdmin";}<div class="backdrop" id="backdrop" style="z-index:140"></div><div id="loader-container" class="vh-100 align-items-center justify-content-center" style="z-index:150">    <div class="loader" style="z-index:150"></div></div><div class="container">    <div class=" mt-2 d-flex justify-content-between align-items-center">                  <h4>Add Business</h4>           <a onclick="Partners()" class="btn shadow  btn-outline-info px-3 ">            <i class="fa-solid fa-angle-left fs-6"></i> Back        </a>    </div>    <div class="shadow border rounded my-3">        <div class="mx-3 my-4 ">            <form method="post" id="SubmitInfoForm" asp-action="AddBusiness" >                               <div class="mt-3">                    <h5 class="my-4">Submit Information</h5>                    <div class="row">                        <div class="mb-3 col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating  ">                            <input type="text" class="form-control" id="floatingInputBusinessname" placeholder="Business Name" asp-for="@Model.BusinessName" aria-label="Business Name">                            <label class="ms-2 text-secondary" for="floatingInputBusinessname">Business Name</label>                       <span class="text-danger" asp-validation-for="@Model.BusinessName"></span>                        </div>                        <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3 ">                            <select class="form-select form-control" id="Profession" asp-for="@Model.SelectedhealthprofID">                                <option value="" disabled selected>Select Profession</option>                                @foreach (var obj in Model.Professions.ToList())
                                {                                    <option value="@obj.HealthProfessionalId">                                        @obj.ProfessionName                                    </option>                                }                            </select>                            <label for="Profession" class="ms-2 text-secondary">Profession</label>                             <span class="text-danger" asp-validation-for="@Model.SelectedhealthprofID"></span>                        </div>                        <div class=" col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3">                            <input type="text" class=" form-control " id="FAXNumber" asp-for="@Model.FAXNumber" placeholder="FAXNumber" aria-label="FAXNumber">                            <span class="text-danger"></span>                            <label class="ms-2 text-secondary" for="FAXNumber">FAX Number</label>                            <span class="text-danger" asp-validation-for="FAXNumber"></span>                        </div>                        <div class=" col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3">                            <input type="tel" class="form-control" id="BusinessInfoPhone" asp-for="@Model.Phonenumber">                            <span class="text-danger" asp-validation-for="@Model.Phonenumber"></span>                        </div>                        <div class=" col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3">                            <input type="email" class=" form-control " asp-for="@Model.Email" id="ProfileInputemail" placeholder="Email" aria-label="Email">                     <span class="text-danger" asp-validation-for="@Model.Email"></span>                            <label class="ms-2 text-secondary" for="floatingInputemail">Email</label>                        </div>                        <div class="mb-3 col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating  ">                            <input type="text" class="form-control" id="floatingInputUserContact" asp-for="BusinessContact" placeholder="User Contact" aria-label="User Contact">                            <label class="ms-2 text-secondary" for="floatingInputUserContact">Business Contact</label>                      <span class="text-danger" asp-validation-for="@Model.BusinessContact"></span>                        </div>                        <div class=" col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3">                            <input type="text" class="form-control " asp-for="@Model.Street" id="floatingInputStreet" placeholder="Street" aria-label="Street">                    <span class="text-danger" asp-validation-for="@Model.Street"></span>                            <label class="ms-2 text-secondary" for="floatingInputStreet">Street</label>                        </div>                        <div class=" col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3">                            <input type="text" class="form-control " asp-for="@Model.City" id="floatingInputCity" placeholder="City" aria-label="City">                        <span class="text-danger" asp-validation-for="@Model.City"></span>                            <label class="ms-2 text-secondary" for="floatingInputCity">City</label>                        </div>                        <div class=" col-xl-6 col-lg-6 col-md-6 col-sm-12 mb-3">                        <select class="form-select regionDropdown" id="state" aria-label="Region" asp-for="@Model.RegionId" style="height: 58px;">                                <option selected="" disabled>State</option>                                @foreach (var obj in Model.regions.ToList())
                                {                                    <option value="@obj.RegionId">                                        @obj.Name                                    </option>                                }                            </select>                       <span class="text-danger" asp-validation-for="@Model.RegionId"></span>                        </div>                        <div class=" col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3">                            <input type="text" class="form-control " id="floatingInputZip" placeholder="Zip" aria-label="Zip" asp-for="@Model.Zip">                           <span class="text-danger" asp-validation-for="@Model.Zip"></span>                            <label class="ms-2 text-secondary" for="floatingInputZip">Zip/Postal</label>                        </div>                    </div>                </div>                <div class="d-grid gap-2 d-flex justify-content-center justify-content-md-end mb-3">                                       <button type="submit" id="savebtn" class="btn btn-info text-white me-1">Save</button>           
                                     <button type="reset" id="cancelBtn" class="btn btn-outline-info CancelBtn">Cancel</button>                </div>            </form>        </div>    </div></div><partial name="_ValidationScriptsPartial" /><style>    .iti {        display: block;    }    .iti__country-list {        z-index: 10;    }    .swal2-popup {        font-size: 15px !important;    }    #BusinessInfoPhone {        height: 58px !important;    }</style><script>    var BusinessInfoPhone = document.querySelector("#BusinessInfoPhone");    var PartnersphoneInput = window.intlTelInput(BusinessInfoPhone, {        utilsScript:            "https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.8/js/utils.js",    });</script>