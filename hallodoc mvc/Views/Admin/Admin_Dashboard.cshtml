    var regid = 0;
    var reqtype = 0;
    var s = '';
    var state=1;
    var page=0;

    const GetData = () => {
        const states = document.getElementsByClassName("border");

        for (let i = 0; i < states.length; ++i) {
            if (states[i].classList.contains("active")) {
                state=states[i].getAttribute('value');
                console.log(states[i].classList[states[i].classList.length - 2])
                $.ajax({
                    method: "POST",
                    url: `/Admin/${states[i].classList[states[i].classList.length - 2]}`,


                    success: function (result) {
                        $('#tableContainer').html(result);
                        $('#all').click();
                        $('#allaccord').click();
                    },

                    error: function () {
                        alert('Error loading partial view');
                    }
                });
            }
        }
    }


    const abc = (e) => {

        document.getElementById("requestor").value = e
        filter();
    }
    const xyz = (e) => {
        document.getElementById("page").value = e
        page=e;
        filter();
    }

 
    const filter = () => {
        const states = document.getElementsByClassName("border");
        console.log(document.getElementById("requestor").value)
        for (let i = 0; i < states.length; ++i) {
            if (states[i].classList.contains("active")) {
                console.log(`${states[i].classList[states[i].classList.length - 2]}`)

                regid = document.getElementById("region").value;
                reqtype = document.getElementById("requestor").value;
                s = document.getElementById("search").value;

                $.ajax({
                    url: `/Admin/${states[i].classList[states[i].classList.length - 2]}`,
                    type: 'POST',
                    dataType: 'html',
                    data: { search: document.getElementById("search").value, region: document.getElementById("region").value, requestor: document.getElementById("requestor").value, pageid:page },
                    // Pass your data here
                    success: function (response) {
                        $("#tableContainer").html(response)
                    },
                    error: function () {
                        alert('An error occurred.');
                    }
                });
            }
        }
    }

    function Export(){
        window.location.href = '/Admin/Export?reqtype=' + reqtype + '&regid=' + regid+'&s='+s+'&state='+state;
    }

    window.onload = () => {
        $.ajax({
            url: `/Admin/New`,
            type: 'POST',
