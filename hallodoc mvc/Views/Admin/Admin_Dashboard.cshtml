<script>
 
    document.addEventListener("DOMContentLoaded", ()=>{
        changeactive();
    });
    function changeactive(){

        if (document.getElementById("new-tab").classList.contains("active")) {
            document.getElementById("onactive").classList.remove("d-none");
            document.getElementById("offactive").classList.add("d-none");
            //document.getElementById("pendingoffactive").classList.remove("d-none");
        } else {
            document.getElementById("offactive").classList.remove("d-none");
            document.getElementById("onactive").classList.add("d-none");
        }
        if (document.getElementById("Pending-tab").classList.contains("active")) {
            document.getElementById("pendingactive").classList.remove("d-none");
            document.getElementById("pendingoffactive").classList.add("d-none");
        } else {
            document.getElementById("pendingoffactive").classList.remove("d-none");
            document.getElementById("pendingactive").classList.add("d-none");
        }
        if (document.getElementById("Active-tab").classList.contains("active")) {
            document.getElementById("Activeactive").classList.remove("d-none");
            document.getElementById("Activeoffactive").classList.add("d-none");
        } else {
            document.getElementById("Activeoffactive").classList.remove("d-none");
            document.getElementById("Activeactive").classList.add("d-none");
        }
        if (document.getElementById("Conclude-tab").classList.contains("active")) {
            document.getElementById("Concludeactive").classList.remove("d-none");
            document.getElementById("Concludeoffactive").classList.add("d-none");
        } else {
            document.getElementById("Concludeoffactive").classList.remove("d-none");
            document.getElementById("Concludeactive").classList.add("d-none");
        }
        if (document.getElementById("ToClose-tab").classList.contains("active")) {
            document.getElementById("Closeactive").classList.remove("d-none");
            document.getElementById("Closeoffactive").classList.add("d-none");
        } else {
            document.getElementById("Closeoffactive").classList.remove("d-none");
            document.getElementById("Closeactive").classList.add("d-none");
        }
        if (document.getElementById("Unpaid-tab").classList.contains("active")) {
            document.getElementById("Unpaidactive").classList.remove("d-none");
            document.getElementById("Unpaidoffactive").classList.add("d-none");
        } else {
            document.getElementById("Unpaidoffactive").classList.remove("d-none");
            document.getElementById("Unpaidactive").classList.add("d-none");
        }

    document.querySelectorAll('.Box').forEach(element=>{
       
            if(element.classList.contains('active')){
                console.log('active');
                var value= element.getAttribute('value');
                if(value==1){
                    document.getElementById(`tdown${value}`).style.borderTopColor = "rgba(2, 2, 146)";
                }
                else if (value == 2) {
                    document.getElementById(`tdown${value}`).style.borderTopColor = " rgb(0, 92, 104)";
                }
                else if(value==3){
                    document.getElementById(`tdown${value}`).style.borderTopColor = "rgb(2, 54, 37)";

                }
                else if (value==4){
                    document.getElementById(`tdown${value}`).style.borderTopColor = " rgb(99, 3, 91)";

                }
                else if(value==5){

                    document.getElementById(`tdown${value}`).style.borderTopColor = " rgb(3, 47, 83)";


                }
                else{
                    document.getElementById(`tdown${value}`).style.borderTopColor = " rgb(44, 1, 70)";

                }
                document.getElementById(`tdown${value}`).classList.remove('d-none');
            }
            else{
                console.log("incactive");
                var value = element.getAttribute('value');
                document.getElementById(`tdown${value}`).classList.add('d-none');
            }
       
    })
    }
