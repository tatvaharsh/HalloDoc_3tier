@model hallodoc_mvc_Repository.ViewModel.ModalData<div class="modal fade" id="myModalSendAgreement" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">    <div class="modal-dialog modal-dialog-centered" role="document">        <div class="modal-content">            <div class="modal-header bg-info">                <div class="modal-title text-light" id="exampleModalLongTitle">Send Agreement</div>                <button type="button" class="btn-close" aria-label="Close" data-bs-dismiss="modal"></button>            </div>            <form class="p-3" asp-action="SendAgreement" method="post">                <div class="d-flex text-secondary">                    <i class="bi bi-circle-fill " id="icon"></i>                    <div class="fs-6 mx-2"><small id="requestor"></small> </div>                </div>                <p>To Send Agreement please make sure you are updating the correct contact information below for the responsible party.</p>                             <div class="mt-4 form-floating text-secondary">                    <input type="text" class="form-control" value="@Model.number" id="floatingInputGrid" placeholder="7894561320">                    <label for="floatingInputGrid">Phone</label>                </div>                <div class="mt-4 form-floating text-secondary">                    <input type="email" class="form-control" value="@Model.email" id="floatingInputGrid" placeholder="name@example.com">                    <label for="floatingInputGrid">Email</label>                </div>                <div class="mt-4 d-flex justify-content-end">                    <button type="submit" class="btn btn-info continue-btn mx-2 text-white">Send</button>                    <button type="button" class="btn cancel-btn btn-outline-info text-info" data-bs-dismiss="modal">Cancel</button>                </div>            </form>        </div>    </div></div><script>    var typedata = @ViewBag.requestType;    console.log(typedata);    switch (typedata) {        case 1:            $('#icon').addClass('pink-icon');            $('#requestor').text('Business');            break;        case 2:            $('#icon').addClass('green-icon');            $('#requestor').text('Patient');            break;        case 3:            $('#icon').addClass('orange-icon');            $('#requestor').text('Family/Friend');            break;        case 4:            $('#icon').addClass('blue-icon');            $('#requestor').text('Concierge');            break;        default:            $('#icon').addClass('pink-icon');            $('#requestor').text('Business');            break;    }</script>