<div class="modal fade" id="createshift" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">    <div class="modal-dialog modal-dialog-centered">        <div class="modal-content">            <div class="modal-header bg-info text-white">                <h1 class="modal-title fs-5" id="exampleModalLabel">Confirm Block</h1>                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>            </div>            <form asp-action="">                <div class="modal-body">                    <select class="form-select mb-3" id="select" onchange="getphy(this.value)" aria-label="Default select example" required>                        <option selected disabled value="">Select a Region</option>                        @* @foreach (var item in Model.Region)                        {                        <option value="@item.RegionId">@item.Name</option>                        } *@                    </select>                    <select class="form-select mb-3" id="physicians" aria-label="Default select example" required>                        <option selected disabled value="">Select a Physician</option>                        @* <option selected>Select Physician</option>                        @foreach (var item in Model.PhysicianName)                        {                        <option value="@item.PhysicianId">@item.FirstName</option>                        } *@                    </select>                    <div class="form-floating mb-3">                        <input type="date" class="form-control" id="shiftdate" min=@DateTime.Now.ToString("yyyy-MM-dd") />                        <label for="shiftdate">Shift Date</label>                    </div>                    <div class="row">                        <div class="col-lg-6 ">                            <div class="form-floating mb-3">                                <input type="time" class="form-control" id="start" />                                <label for="start">Start</label>                            </div>                        </div>                        <div class="col-lg-6 ">                            <div class="form-floating mb-3 w-100">                                <input type="time" class="form-control" id="end" />                                <label for="end">End</label>                            </div>                        </div>                    </div>                    <div class="form-check form-switch">                        <label class="form-check-label" for="repeat">Repeat</label>                        <input class="form-check-input" type="checkbox" id="repeat" />                    </div>                    <span class="mt-3">Repeat Days</span>                    <fieldset class="row mt-2 mb-3">                        <div class="col-5">                            <input class="form-check-input" type="checkbox" value="" id="sunday">                            <label class="form-check-label" for="sunday">Every Sunday</label>                        </div>                        <div class="col-5">                            <input class="form-check-input" type="checkbox" value="" id="monday">                            <label class="form-check-label" for="monday">Every Monday</label>                        </div>                        <div class="col-5">                            <input class="form-check-input" type="checkbox" value="" id="tuesday">                            <label class="form-check-label" for="tuesday">Every Tuesday</label>                        </div>                        <div class="col-5">                            <input class="form-check-input" type="checkbox" value="" id="wednesday">                            <label class="form-check-label" for="wednesday">Every Wednesday</label>                        </div>                        <div class="col-5">                            <input class="form-check-input" type="checkbox" value="" id="thursday">                            <label class="form-check-label" for="thursday">Every Thursday</label>                        </div>                        <div class="col-5">                            <input class="form-check-input" type="checkbox" value="" id="friday">                            <label class="form-check-label" for="friday">Every Friday</label>                        </div>                        <div class="col-5">                            <input class="form-check-input" type="checkbox" value="" id="saturday">                            <label class="form-check-label" for="saturday">Every Saturday</label>                        </div>                    </fieldset>                    <div class="form-floating">                        <select class="form-select mb-3" id="repeatamt" aria-label="Default select example">                            <option selected value="1">1</option>                            <option value="2">2</option>                            <option value="3">3</option>                            <option value="4">4</option>                            <option value="5">5</option>                        </select>                        <label for="repeatamt">Repeat</label>                    </div>                </div>                <div class="modal-footer">                    <button type="submit" class="btn btn-outline-info mx-1 back-btn logout-btn"> Save </button>                    <button type="button" class="btn btn-outline-info mx-1 back-btn logout-btn" data-bs-dismiss="modal">                        Cancel                    </button>                </div>            </form>        </div>    </div></div>