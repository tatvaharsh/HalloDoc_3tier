@model AssignmentRepository.ViewModel.UserModel<div class="modal fade" id="createshift" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true" >    <div class="modal-dialog modal-dialog-centered" >        <div class="modal-content" >            <div class="modal-header text-white" style="background-color:rgb(221,160,221);">                <h1 class="modal-title fs-5" id="exampleModalLabel">User Form</h1>                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>            </div>            <form asp-action="AddUser" >                <div class="modal-body">                    <div class="row mb-3">                        <div class="col-12 col-md-6 form-floating mb-3">                            <input type="text" class="form-control bg-transparent" id="floatingInputFName" placeholder="Firstname" asp-for="FirstName">                            <label for="floatingInputFName" class="ms-2">First Name</label>                            <span asp-validation-for="FirstName" class="text-danger"></span>                        </div>                        <div class="col-12 col-md-6 form-floating mb-3">                            <input type="text" class="form-control bg-transparent" id="floatingInputLName" placeholder="LastName" asp-for="LastName">                            <label for="floatingInputLName" class="ms-2">Last Name</label>                            <span asp-validation-for="LastName" class="text-danger"></span>                        </div>                        <div class="col-12 col-md-6 form-floating mb-3">                            <input type="email" class="form-control bg-transparent" id="floatingInputEmail" placeholder="name@example.com" asp-for="Email">                            <label for="floatingInputEmail" class="ms-2">Email</label>                            <span asp-validation-for="Email" class="text-danger"></span>                        </div>                        <div class="col-12 col-md-6 form-floating mb-3">                            <input type="date" class="form-control bg-transparent" id="datePickerId" placeholder="DateOfBirth" asp-for="BirthDate">                            <label for="datePickerId" class="ms-2">Date Of Birth</label>                            <span asp-validation-for="BirthDate" class="text-danger"></span>                        </div>                        <div class="col-12 col-md-6 form-floating mb-3">                        Gender :                         <div class="form-check col-2">                            <input class="form-check-input" type="radio" value="1" name="Gendermale" id="flexRadioDefault1" asp-for="Gendermale">                            <label class="form-check-label" for="flexRadioDefault1">                               Male                            </label>                        </div>                        <div class="form-check col-2">                            <input class="form-check-input" type="radio" name="Gendermale" value="2" id="flexRadioDefault2" asp-for="Gendermale">                            <label class="form-check-label" for="flexRadioDefault2">                                Female                            </label>                        </div>                        <div class="form-check col-2">                            <input class="form-check-input" type="radio" name="Gendermale" value="3" id="flexRadioDefault2" asp-for="Gendermale">                            <label class="form-check-label" for="flexRadioDefault2">                                Other                            </label>                        </div>                            <span asp-validation-for="Genderfemale" class="text-danger"></span>                        </div>                                                   <div class="col-12 col-md-6 form-floating mb-3">                            <input  type="tel" class="form-control bg-transparent" id="floatingInputPhone" placeholder="number" asp-for="PhoneNumber">                            <label for="floatingInputPhone" class="ms-2">Phone Number</label>                            <span asp-validation-for="PhoneNumber" class="text-danger"></span>                        </div>                        <select class="form-select col-md-6  mb-3" aria-label=".form-select-lg example" asp-for="Country" style="height:calc(3.5rem + 2px); width:50%; ">                            <option selected>Country</option>                            <option value="1">India</option>                            <option value="2">U.K</option>                            <option value="3">U.S.A</option>                        </select>                        <div class="col-12 col-md-6 form-floating mb-3 ui-widget ui-front">                            <input type="text" class="form-control bg-transparent" id="tags" placeholder="LastName" asp-for="City">                            <label for="floatingInputLName" class="ms-2">City</label>                            <span asp-validation-for="City" class="text-danger"></span>                        </div>                    </div>                                                   </div>                <div class="modal-footer">                    <button type="button" class="btn btn-outline-info mx-1 back-btn logout-btn" data-bs-dismiss="modal">                        Cancel                    </button>                    <button type="submit" id="btn3" class="btn btn-outline-info mx-1 back-btn logout-btn"> Save </button>                                </div>            </form>        </div>    </div></div><script>    $(async function () {        $("#tags").autocomplete({            source: await fetch('/home/citynames').then(r=>r.json())        });    });</script><script>
</script><script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script><script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>